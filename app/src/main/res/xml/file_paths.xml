<?xml version="1.0" encoding="utf-8"?>
<paths>

    <!--升级-->
    <external-cache-path
        name="mn_crash_external_cache"
        path="." />

    <cache-path
        name="mn_crash_cache"
        path="." />

    <external-path
        name="mn_crash_external_path"
        path="." />

    <files-path
        name="mn_crash_files_path"
        path="." />

    <external-files-path
        name="mn_crash_external_files_path"
        path="." />

    <root-path
        name="mn_crash_root_path"
        path="." />
    <!--物理路径相当于Context.getFilesDir() + /path/-->
    <files-path name="name" path="path" />

    <!--物理路径相当于Context.getCacheDir() + /path/-->
    <cache-path name="name" path="path" />
    <!-- 物理路径相当于Environment.getExternalStorageDirectory() + /path/-->
    <external-path name="name" path="path" />
    <!-- 物理路径相当于Context.getExternalFilesDir(String) + /path/-->
    <external-files-path name="name" path="path" />
    <!-- 物理路径相当于Context.getExternalCacheDir() + /path/-->
    <external-cache-path name="name" path="path" />
</paths>